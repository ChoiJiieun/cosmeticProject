<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<!-- Oracle 테이블의 컬럼이 mem_id 이고, DTO의 필드변수가 memId 일때 매칭이 가능하게 하는 옵션 -->
		<setting name="mapUnderscoreToCamelCase" value="true"/>
		<!-- 널 허용 -->
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>
	<typeAliases>
		<typeAlias type="comm.proj.my.member.vo.MemberVO" alias="MemberVO" />
		<typeAlias type="comm.proj.my.member.vo.SeasonRecordVO" alias="SeasonRecordVO" />
		<typeAlias type="comm.proj.my.member.vo.SeasonDetailVO" alias="SeasonDetailVO" />
		<typeAlias type="comm.proj.my.member.vo.SeasonInfoVO" alias="SeasonInfoVO" />
		<typeAlias type="comm.proj.my.member.vo.AddInfoVO" alias="AddInfoVO" />
		<typeAlias type="comm.proj.my.member.vo.FaceRecordVO" alias="FaceRecordVO" />
		<typeAlias type="comm.proj.my.cosmetic.vo.CosVO" alias="CosVO" />
		<typeAlias type="comm.proj.my.cosmetic.vo.ReviewVO" alias="ReviewVO" />
		<typeAlias type="comm.proj.my.cosmetic.vo.IngredientVO" alias="IngredientVO" />
		<typeAlias type="comm.proj.my.community.vo.CommVO" alias="CommVO" />
		<typeAlias type="comm.proj.my.community.vo.BoardVO" alias="BoardVO" />
		<typeAlias type="comm.proj.my.community.vo.PagingVO" alias="PagingVO" />
		<typeAlias type="comm.proj.my.community.vo.ReplyVO" alias="ReplyVO" />
		<typeAlias type="comm.proj.my.rank.vo.RankVO" alias="RankVO" />
		
	</typeAliases>
</configuration>